<?xml version="1.0"?>
<block>
  <name>debug_block</name>
  <key>tun_blocks_debug_block</key>
  <category>TUN_BLOCKS</category>
  <import>import tun_blocks</import>
  <make>tun_blocks.debug_block()</make>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
